@model WCS.PageModels.PageModel
@{
    ViewBag.Title = "Index";
}
<car-result></car-result>
<h2>Index </h2>
<a href='@Url.Action("IndexAngular", "Home")'>Angular Version</a>
<div class="row">
    <div class="col-md-6">
        <h3>Search Results</h3>
        @if (Model != null)
        {           
            @Html.Partial("CarResults", Model.Results, new ViewDataDictionary { { "action", "Add" }, { "buttonName", "Save" } })
        }
    
    </div>
    <div class="col-md-6">

        <h3>Saved Cars</h3>
        @if (Model != null)
        {   @Html.Partial("CarResults",Model.SavedCars,new ViewDataDictionary { { "action","Remove"}, { "buttonName","Remove"} })
            
        }    
    </div>
</div>


<br/>


